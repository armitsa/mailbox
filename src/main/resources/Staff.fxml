<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #95C0CB;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="staff.StaffController">
   <children>
      <ImageView fitHeight="35.0" fitWidth="35.0" layoutX="750.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@picture/settings.png" />
         </image>
      </ImageView>
      <Button fx:id="settingsBtn" alignment="CENTER" contentDisplay="CENTER" layoutX="750.0" layoutY="15.0" mnemonicParsing="false" onAction="#handleSettingsAction" prefHeight="35.0" prefWidth="35.0" style="-fx-background-color: none;">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="dormBtn" layoutX="162.0" layoutY="261.0" mnemonicParsing="false" onAction="#handleDormBtnAction" prefHeight="75.0" prefWidth="150.0" text="Dorm">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button fx:id="itemBtn" layoutX="524.0" layoutY="261.0" mnemonicParsing="false" onAction="#handleItemBtnAction" prefHeight="75.0" prefWidth="145.0" text="Item">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button fx:id="logoutBtn" layoutX="724.0" layoutY="551.0" mnemonicParsing="false" onAction="#handleLogOutBtnAction" text="Log out" />
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</AnchorPane>
