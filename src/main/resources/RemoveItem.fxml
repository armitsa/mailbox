<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #95C0CB;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="staff.item.RemoveItemController">
   <children>
      <Label layoutX="44.0" layoutY="47.0" text="Remove Parcel" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <HBox layoutX="83.0" layoutY="123.0" prefHeight="421.0" prefWidth="618.0">
         <children>
            <AnchorPane prefHeight="422.0" prefWidth="790.0" style="-fx-background-color: white;">
               <children>
                  <Label layoutX="120.0" layoutY="90.0" text="Building :">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="build" layoutX="225.0" layoutY="90.0" promptText="Enter build" />
                  <Label layoutX="120.0" layoutY="150.0" text="Floor :">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="floor" layoutX="225.0" layoutY="150.0" promptText="Enter Room" />
                  <Label layoutX="120.0" layoutY="210.0" text="Room :">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="room" layoutX="225.0" layoutY="210.0" promptText="Enter room" />
                  <Button fx:id="removeBtn" layoutX="279.0" layoutY="306.0" mnemonicParsing="false" onAction="#handleRemoveBtnAction" text="Remove" />
               </children>
            </AnchorPane>
         </children>
      </HBox>
      <Button fx:id="backBtn" layoutX="745.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackBtnAction" text="Back" />
   </children>
</AnchorPane>
