<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #95C0CB;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="account.ChangePasswordController">
   <children>
      <Button fx:id="backBtn" layoutX="736.0" layoutY="25.0" mnemonicParsing="false" onAction="#handleBackBtnAction" text="Back" />
      <HBox layoutX="271.0" layoutY="250.0" prefHeight="100.0" prefWidth="200.0" />
      <Label layoutX="88.0" layoutY="51.0" text="Change Password" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <HBox layoutX="50.0" layoutY="111.0" prefHeight="439.0" prefWidth="705.0">
         <children>
            <AnchorPane prefHeight="439.0" prefWidth="803.0" style="-fx-background-color: white;">
               <children>
                  <Label layoutX="100.0" layoutY="100.0" text="Username :" />
                  <Label layoutX="100.0" layoutY="150.0" text="New Password :" />
                  <Label layoutX="100.0" layoutY="200.0" text="Confirm password :" />
                  <TextField fx:id="username" layoutX="280.0" layoutY="100.0" promptText="Enter Username" />
                  <PasswordField fx:id="password" layoutX="280.0" layoutY="150.0" promptText="Enter password" />
                  <PasswordField fx:id="confirm" layoutX="280.0" layoutY="196.0" promptText="Confirm" />
                  <Button fx:id="acceptBtn" defaultButton="true" layoutX="327.0" layoutY="364.0" mnemonicParsing="false" onAction="#handleAcceptBtnAction" text="Accept" />
               </children>
            </AnchorPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
